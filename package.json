{
  "name": "react-agile",
  "version": "1.0.0",
  "description": "React server side rendering with react router, express, styled components",
  "scripts": {
    "start:server": "rm -rf ./build && webpack --config  webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "rm -rf ./.build && npm-run-all --parallel start:server start:client"
  },
  "homepage": "https://github.com/arrlancore/react-agile",
  "bugs": {
    "url": "https://github.com/arrlancore/react-agile/issues"
  },
  "keywords": [
    "react",
    "redux",
    "styled-components",
    "express",
    "nodejs",
    "webpack",
    "hmr",
    "babel"
  ],
  "author": "Dwiki Arlan",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-react": "^6.23.0",
    "express": "^4.14.1",
    "npm-run-all": "^4.0.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "next",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "start-server-webpack-plugin": "^2.1.2",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "webpack": "3.0.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "2.11.1",
    "webpack-node-externals": "^1.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2017": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "import-inspector": "^2.0.0"
  }
}
